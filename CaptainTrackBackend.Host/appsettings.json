{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "CaptainTrackConnectionString": "YOUR_CONNECTION_STRING_HERE"
  },
  "Jwt": {
    "Key": "YOUR_JWT_SECRET_KEY_HERE_MINIMUM_32_CHARACTERS",
    "Issuer": "CaptainTrack",
    "Audience": "CaptainTrackUser"
  },
  "SendGrid": {
    "ApiKey": "YOUR_SENDGRID_API_KEY_HERE",
    "FromEmail": "noreply@yourdomain.com"
  },
  "SmtpSettings": {
    "Host": "smtp.gmail.com",
    "Port": "587",
    "Username": "YOUR_EMAIL_HERE",
    "Password": "YOUR_APP_PASSWORD_HERE",
    "FromEmail": "YOUR_EMAIL_HERE"
  },
  "Flutterwave": {
    "SecretKey": "YOUR_FLUTTERWAVE_SECRET_KEY",
    "PublicKey": "YOUR_FLUTTERWAVE_PUBLIC_KEY",
    "BaseUrl": "https://api.flutterwave.com/v3"
  },
  "GoogleMaps": {
    "ApiKey": "YOUR_GOOGLE_MAPS_API_KEY"
  },
  "CloudinarySettings": {
    "CloudName": "YOUR_CLOUDINARY_CLOUD_NAME",
    "ApiKey": "YOUR_CLOUDINARY_API_KEY",
    "ApiSecret": "YOUR_CLOUDINARY_API_SECRET"
  },
  "Hollatags": {
    "User": "YOUR_HOLLATAGS_USER",
    "Pass": "YOUR_HOLLATAGS_PASSWORD",
    "From": "Captaint",
    "CallbackUrl": "https://yourdomain.com/api/sms/callback"
  },
  "Firebase": {
    "DatabaseUrl": "YOUR_FIREBASE_DATABASE_URL",
    "ServiceAccountKeyPath": "serviceAccountKey.json"
  },
  "Cors": {
    "AllowedOrigins": [
      "https://yourdomain.com",
      "https://app.yourdomain.com"
    ]
  }
}
